{
  "filters": [
    {
      "name": "AWS Lambda Errors",
      "tags": ["AWS", "Lambda"],
      "rules": ["aws-lambda-timeout", "aws-lambda-early-exit", "aws-lambda-out-of-memory"]
    },{
      "name": "Node.js errors",
      "tags": ["Node.js"],
      "rules": ["node-error", "basic-node-error"]
    }
  ],
  "rules": [
    {
      "id": "aws-lambda-timeout",
      "service": "AWS Lambda",
      "name": "TIMEOUT",
      "type": "REGEX",
      "pattern": "/[^\\s]{2}\\sTask timed out after/"
    },{
      "id": "aws-lambda-early-exit",
      "service": "AWS Lambda",
      "name": "EARLY_EXIT",
      "type": "REGEX",
      "pattern": "/[^\\s]{2}\\sProcess exited before completing request/"
    },{
      "id": "aws-lambda-out-of-memory",
      "service": "AWS Lambda",
      "name": "OUT_OF_MEMORY",
      "type": "GLOB",
      "pattern": "REPORT * | 'Memory Size: *' 'Max Memory Used: *' | memSize, memUsed | where memSize == memUsed"
    }, {
      "id": "aws-lambda-out-of-memory",
      "service": "AWS Fargate",
      "name": "test",
      "type": "GLOB",
      "pattern": "REPORT * | 'Memory Size: *' 'Max Memory Used: *' | memSize, memUsed | where memSize == memUsed"

    },{
      "id": "node-error",
      "service": "Node.js",
      "name": "ERROR",
      "type": "GLOB",
      "pattern": "Syntax error in module*"
    },{
      "id": "basic-node-error",
      "service": "Node.js",
      "name": "BASIC_ERROR",
      "type": "GLOB",
      "pattern": "*Task timed out*"
    }
  ]
}
